# Test file to find instances of foo

# TEST: foo as function name
def foo():
    # TEST: foo inside a longer string
    print("This is a string with foo")
    # TEST: foo on its own
    print("foo")
    return "FOO"

# TEST: foo in function name
def not_foo():
    return "NOT FOO"

if __name__ == "__main__":
    print(foo)
    print(not_foo)
    print(foo())
    print(not_foo())
